            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "ASSET_SUB_TYPE_UNKNOWN",
              "ASSET_SUB_TYPE_LORAWAN_GATEWAY_CPI_ODU",
              "ASSET_SUB_TYPE_LORAWAN_GATEWAY_CPI_IDU",
              "ASSET_SUB_TYPE_WATER_METER_AMR_INDUCTIVE_AIL_V1",
